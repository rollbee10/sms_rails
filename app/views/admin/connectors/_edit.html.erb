<%= form_for(@connector, url: admin_connector_path(@connector), html: {class: "form-horizontal", role:"form"}) do |f| %>
    <section class="panel">
      <header class="panel-heading">
        <h2 class="panel-title">Edit Connector</h2>
      </header>
      <div class="panel-body">
        <div class="col-md-offset-2 col-md-8">
          <div class="form-group">
            <label class="col-sm-3 control-label">CID</label>
            <div class="col-sm-9">
              <%= f.text_field :cid, class: 'form-control', required:'true', placeholder:'' %>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-3 control-label">Host</label>
            <div class="col-sm-9">
              <%= f.text_field :host, class: 'form-control', required:'true', placeholder:'' %>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-3 control-label">Port</label>
            <div class="col-sm-9">
              <%= f.text_field :port, class: 'form-control', required:'true', placeholder:'' %>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-3 control-label">Username</label>
            <div class="col-sm-9">
              <%= f.text_field :username, class: 'form-control', required:'true', placeholder:'' %>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-3 control-label">Password</label>
            <div class="col-sm-9">
              <%= f.text_field :password, class: 'form-control', required:'true', placeholder:'' %>
            </div>
          </div>
          <div class="form-group">
            <label class="col-sm-2 control-label"></label>
            <div class="col-sm-9">
              <%= f.submit 'Update', class: 'btn btn-primary' %>
              <%= link_to 'Back',  admin_connectors_path , class: 'btn btn-default' %>
            </div>
          </div>
        </div>
      </div>
    </section>
<% end %>
